# NewVisionWellness - Environment Configuration for HIPAA-Compliant DynamoDB

# Copy this file to .env and fill in your actual values
# NEVER commit .env files to version control

# =====================================
# AWS CONFIGURATION
# =====================================

# AWS Credentials (Use IAM role or user with minimal required permissions)
VITE_AWS_ACCESS_KEY_ID=your_access_key_here
VITE_AWS_SECRET_ACCESS_KEY=your_secret_key_here
VITE_AWS_REGION=us-west-2

# AWS KMS Key for encryption (recommended for production)
VITE_AWS_KMS_KEY_ID=your_kms_key_id_here

# Client-side encryption key (fallback - use KMS in production)
VITE_ENCRYPTION_KEY=your_32_character_encryption_key_here

# =====================================
# DYNAMODB CONFIGURATION
# =====================================

# Table names (these match the setup script)
VITE_INSURANCE_TABLE=NewVisionWellness_InsuranceVerifications
VITE_CLIENT_DATA_TABLE=NewVisionWellness_ClientData
VITE_TREATMENT_RECORDS_TABLE=NewVisionWellness_TreatmentRecords
VITE_SESSIONS_TABLE=NewVisionWellness_Sessions
VITE_AUDIT_TABLE=NewVisionWellness_AuditLog
VITE_USER_REGISTRATIONS_TABLE=NewVisionWellness_UserRegistrations

# =====================================
# HIPAA COMPLIANCE SETTINGS
# =====================================

# Session timeout (in milliseconds)
VITE_SESSION_TIMEOUT=86400000

# Data retention period (in days)
VITE_DATA_RETENTION_DAYS=2555

# Audit log retention (in days)
VITE_AUDIT_RETENTION_DAYS=2555

# Enable additional security logging
VITE_ENABLE_AUDIT_LOGGING=true

# =====================================
# APPLICATION SETTINGS
# =====================================

# Environment (development, staging, production)
VITE_ENVIRONMENT=development

# Application URL
VITE_APP_URL=http://localhost:5173

# Contact information
VITE_CONTACT_PHONE=8186008640
VITE_CONTACT_EMAIL=admissions@newvisionwellness.com

# =====================================
# SECURITY SETTINGS
# =====================================

# Enable HTTPS (required for production)
VITE_FORCE_HTTPS=true

# Enable Content Security Policy
VITE_CSP_ENABLED=true

# Rate limiting (requests per minute)
VITE_RATE_LIMIT=60

# =====================================
# INTEGRATION SETTINGS
# =====================================

# Google AI API Key (for insurance verification)
VITE_GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Other service API keys
VITE_STRIPE_PUBLIC_KEY=your_stripe_public_key_here
VITE_TWILIO_ACCOUNT_SID=your_twilio_account_sid_here

# =====================================
# DEVELOPMENT SETTINGS
# =====================================

# Debug mode (disable in production)
VITE_DEBUG_MODE=false

# Enable development tools
VITE_DEV_TOOLS=true

# Mock data for testing
VITE_USE_MOCK_DATA=false
